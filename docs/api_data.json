  {
    "type": "post",
    "url": "/music/similar",
    "title": "Get similar songs",
    "version": "0.1.0",
    "name": "Similar_songs",
    "group": "Music",
    "parameter": {
      "fields": {
        "Parameter": [
          {
            "group": "Parameter",
            "type": "String",
            "optional": false,
            "field": "track_id",
            "description": "<p>The spotify track_id unique to each song.</p>"
          },
          {
            "group": "Parameter",
            "type": "Number",
            "optional": true,
            "field": "number",
            "description": "<p>_like The number of songs you'd like returned. Defaults to 100 similar songs.</p>"
